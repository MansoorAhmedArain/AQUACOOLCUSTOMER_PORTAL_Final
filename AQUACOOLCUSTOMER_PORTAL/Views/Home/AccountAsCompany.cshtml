                        <div class="input-style">

                            <span class="ms-3" style="color: #03213d; font-size: small; font-weight: bolder;">Company Name (as per trade license)</span>
                            <div class="input-group">
                                <input type="text" placeholder="Required" name="Company" required class="form-control rounded-pill" style="border: 1px solid #8FD2F0;">
                            </div>

                            <span class="ms-3" style="color: #03213d; font-size: small; font-weight: bolder;">Tax Registration Number (VAT number)</span>
                            <div class="input-group">
                                <input type="text" placeholder="Required" name="TRN" required class="form-control rounded-pill" style="border: 1px solid #8FD2F0;">
                            </div>

                            <span class="ms-3" style="color: #03213d; font-size: small; font-weight: bolder;">Trade License Number</span>
                            <div class="input-group">
                                <input type="text" placeholder="Required" name="TradeLicenseNumber" required class="form-control rounded-pill" style="border: 1px solid #8FD2F0;">
                            </div>

                            <span class="ms-3" style="color: #03213d; font-size: small; font-weight: bolder;">Trade License Expiry Date</span>
                            <div class="input-group">
                                <span class="input-group-text rounded-pill rounded-end"><img src="~/Icons/Date.png" alt="" class="icon-image"></span>
                                <input type="date" placeholder="9/5/2024" name="TradeLicenseExpiryDate" class="form-control rounded-start rounded-pill" style="border: 1px solid #8FD2F0;">
                            </div>

                            <span class="ms-3" style="color: #03213d; font-size: small; font-weight: bolder;">First Name</span>
                            <div class="input-group">
                                <input type="text" placeholder="Required" name="FirstName" required class="form-control rounded-pill" style="border: 1px solid #8FD2F0;">
                            </div>


                            <span class="ms-3" style="color: #03213d; font-size: small; font-weight: bolder;">Last Name</span>
                            <div class="input-group">
                                <input type="text" placeholder="Required" name="LastName" required class="form-control rounded-pill" style="border: 1px solid #8FD2F0;">
                            </div>

                            <span class="ms-3" style="color: #03213d; font-size: small; font-weight: bolder;">Nationality</span>
                            <div class="input-group">
                                @*  <input type="text" name="Nationality" required class="form-control rounded-pill" style="border: 1px solid #8FD2F0;"> *@
                                @Html.DropDownListFor(model => model.Nationality, (IEnumerable<SelectListItem>)ViewBag.Countries, "Select Country", new { @class = "form-control rounded-pill", @style = "border: 1px solid #8FD2F0;" })
                            </div>

                            <div class="btn-container my-3">
                                <button type="button" class="btn rounded-pill backbtn-style prev-step">Back</button>
                                <button type="button" class="btn rounded-pill btn-style next-step">Next</button>
                            </div>
                        </div>


                    </div>


                    <div class="step step-2">
                        <!-- Step 2 form fields here -->

